
{
  "name": "burden-api-gui",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server",
    "build": "webpack --config webpack.config.js --progress --colors -p",
    "lint": "eslint --quiet --ignore-pattern node_modules/**/*.js --ignore-pattern static/**/*.js .",
    "test": "mocha-webpack"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "eslintConfig": {
    "parser": "babel-eslint",
    "env": {
      "browser": true,
      "node": true,
      "mocha": true,
      "es6": true
    },
    "plugins": [
      "react"
    ],
    "extends": "airbnb",
    "rules": {
      "new-cap": 0,
      "strict": 0,
      "eol-last": 0,
      "no-console": 0,
      "no-undef": 2,
      "no-unused-vars": 2,
      "react/jsx-uses-react": 1,
      "react/jsx-uses-vars": 1,
      "consistent-return": 0,
      "arrow-body-style": 0,
      "no-param-reassign": 0,
      "no-case-declarations": 0,
      "quote-props": 0,
      "prefer-arrow-callback": 0,
      "max-len": 0,
      "prefer-template": 0,
      "prop-types": 0,
      "wrap-iife": 0,
      "react/prop-types": 0,
      "no-confusing-arrow": 0,
      "prefer-const": 0,
      "jsx-a11y/img-uses-alt": 0,
      "no-debugger": 0,
      "react/jsx-indent": 0,
      "jsx-a11y/img-has-alt": 0,
      "no-underscore-dangle": 0,
      "import/no-unresolved": 0,
      "global-require": 0,
      "no-restricted-syntax": 0
    }
  },
  "dependencies": {
    "artsy-elan": "^1.0.1",
    "babel-core": "^6.0.20",
    "babel-eslint": "^4.1.8",
    "babel-loader": "^6.0.1",
